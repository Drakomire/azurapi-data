	"scripts": [
		{
			"actor": 0,
			"side": 0,
			"say": "\u554a\uff0c\u65b0\u4e0a\u4efb\u7684\u7b2c\u4e00\u5929\u5c31\u8ff7\u8def\u4e86\uff0c\u8fd9\u4e48\u4e3b\u89d2\u5149\u73af\u7684\u51fa\u573a\u6a21\u5f0f\u771f\u662f\u5c34\u5c2c\u2026"
		},
		{
			"actor": 0,
			"side": 0,
			"say": "\u4e0d\u8fc7\uff0c\u6309\u7167\u8fd9\u79cd\u5957\u8def\u6765\u770b\uff0c\u63a5\u4e0b\u6765\u96be\u9053\u6211\u5c31\u662f\u8981\u9047\u5230\u547d\u8fd0\u4e2d\u7684\u5973\u4e3b\u89d2\u7684\u8282\u594f\u4e86\u5417\u563f\u563f\u563f..."
		},
		{
			"actor": 201010,
			"side": 1,
			"say": "\u554a\u5566~\u4f60\u5c31\u662f\u65b0\u6765\u7684\u6307\u6325\u5b98\u5417\uff0c\u5c45\u7136\u8ba9\u6dd1\u5973\u7b49\u5f85\u90a3\u4e48\u4e45\u7684\u65f6\u95f4\uff0c\u53ef\u771f\u4e0d\u662f\u4e2a\u5408\u683c\u7684\u7ec5\u58eb\u5462\u54fc\u54fc\uff01"
		},
		{
			"actor": 0,
			"side": 0,
			"say": "\u55ef\uff1f\u54ea\u91cc\u7a81\u7136\u8dd1\u51fa\u6765\u7684\u5c0f\u5b69\u5b50\uff0c\u8fd8\u7a7f\u7740\u8fd9\u6837\u5947\u602a\u7684\u519b\u670d\uff1f"
		},
		{
			"actor": 201010,
			"side": 1,
			"say": "\u4ec0\u2026\u4ec0\u4e48\uff0c\u4eba\u5bb6\u624d\u4e0d\u662f\u54ea\u91cc\u8dd1\u51fa\u6765\u7684\u5c0f\u5b69\u5b50\u5462\uff01\u522b\u770b\u6211\u4e2a\u5934\u5c0f\uff0c\u5728\u6211\u624b\u4e0b\u6559\u5bfc\u51fa\u6765\u7684\u4f18\u79c0\u5b66\u751f\u53ef\u662f\u6570\u4e0d\u80dc\u6570\u5462\uff01"
		},
		{
			"actor": 0,
			"side": 0,
			"say": "\u8bf6\uff01\u4f60\u5c45\u7136\u80fd\u542c\u5230\u6211\u8111\u5b50\u91cc\u5728\u60f3\u7684\u4e1c\u897f\uff01\uff1f"
		},
		{
			"actor": 201010,
			"side": 1,
			"say": "\u54fc\u54fc\uff0c\u6211\u4eec\u53ef\u662f\u642d\u8f7d\u4e86\u6700\u65b0\u578b\u7684\u6218\u672f\u6307\u6325\u7cfb\u7edf\u7684\u201c\u5175\u5668\u201d\u54df\uff0c\u53ea\u8981\u901a\u8fc7\u6307\u6325\u5b98\u7684\u8111\u7535\u6ce2\u5c31\u53ef\u4ee5\u8fdb\u884c\u8fdc\u7a0b\u6c9f\u901a\uff0c\u8fd9\u70b9\u5c0f\u4e8b\u60c5\u6839\u672c\u4e0d\u7b97\u4ec0\u4e48~"
		},
		{
			"actor": 201010,
			"side": 1,
			"say": "\u5f53\u7136\uff0c\u6307\u6325\u5b98\u8111\u5b50\u91cc\u8981\u662f\u5728\u60f3\u4e00\u4e9b\u5f88\u5947\u602a\u7684\u4e1c\u897f\u7684\u8bdd\uff0c\u6211\u4eec\u4e5f\u662f\u80fd\u77e5\u9053\u7684\u54e6\u54fc\u54fc\u54fc~"
		},
		{
			"actor": 201010,
			"side": 1,
			"say": "\u554a\u5566\uff0c\u5fd8\u4e86\u81ea\u6211\u4ecb\u7ecd\u4e00\u4e0b\uff0c\u6211\u53ef\u662f\u6765\u81ea\u7687\u5bb6\u8230\u961f\u7684\u9ad8\u7ea7\u6559\u5b98\u5973\u5c06\u53f7\u54e6\uff0c\u8d1f\u8d23\u5bf9\u65b0\u4eba\uff08\u4f60\uff09\u8fdb\u884c\u57fa\u7840\u6559\u5bfc~\u4ece\u4eca\u5929\u5f00\u59cb\u5c31\u8f6e\u5230\u6211\u6765\u597d\u597d\u8c03\u6559\u4f60\u4e00\u756a\u5566\uff01\u52a0\u52a0\u4f1a\u914d\u5408\u4f60\u505a\u5b9e\u6218"
		},
		{
			"actor": 0,
			"side": 0,
			"say": "\u554a\u2026\u611f\u89c9\u4f3c\u4e4e\u6709\u4e00\u79cd\u4e0d\u7965\u7684\u9884\u611f-A-"
		}
	]
}