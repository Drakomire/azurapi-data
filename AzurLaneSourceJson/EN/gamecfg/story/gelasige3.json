		},
		{
			"side": 2,
			"dir": 1,
			"typewriter": {
				"speed": 0.05,
				"speedUp": 0.01
			},
			"say": "Just as the words left Ardent\u2019s lips...",
			"painting": {
				"alpha": 0.3,
				"time": 1
			},
			"bgName": "bg_story_school"
		},
		{
			"side": 2,
			"dir": 1,
			"typewriter": {
				"speed": 0.05,
				"speedUp": 0.01
