			"bgName": "bg_story_outdoor"
		},
		{
			"actor": 303110,
			"side": 2,
			"dir": 1,
			"nameColor": "#a9f548",
			"expression": 3,
			"say": "Commander, what is she...?",
			"typewriter": {
				"speed": 0.05,
				"speedUp": 0.01
			},
			"painting": {
				"alpha": 0.3,
				"time": 1
			},
			"bgName": "bg_story_outdoor"
		},
		{
			"actor": 206050,
			"side": 2,
			"dir": 1,
			"nameColor": "#a9f548",
			"expression": 3,
			"say": "Oh, dreadfully sorry, I\u2019m a bit too excited. Is that your samurai sword? May I see it?",
			"typewriter": {
				"speed": 0.05,
				"speedUp": 0.01
			},
			"painting": {
				"alpha": 0.3,
				"time": 1
			},
			"bgName": "bg_story_outdoor"
		},
		{
			"actor": 303110,
			"side": 2,
			"dir": 1,
			"nameColor": "#a9f548",
			"expression": 3,
			"say": "I don\u2019t mind showing you, but it is dangerous to handle the naked blade... You should not get too close.",
			"typewriter": {
				"speed": 0.05,
				"speedUp": 0.01
			},
			"painting": {
				"alpha": 0.3,
				"time": 1
			},
			"bgName": "bg_story_outdoor"
		},
		{
