		},
		{
			"side": 2,
			"dir": 1,
			"typewriter": {
				"speed": 0.05,
				"speedUp": 0.01
			},
			"say": "She smiled and turned to face me.",
			"painting": {
				"alpha": 0.3,
				"time": 1
			},
			"bgName": "bg_main_twilight"
		},
		{
			"actor": 206050,
			"side": 2,
			"dir": 1,
			"nameColor": "#a9f548",
			"expression": 1,
			"say": "Now then, why did you ask me here?",
			"typewriter": {
				"speed": 0.05,
				"speedUp": 0.01
			},
			"painting": {
				"alpha": 0.3,
				"time": 1
			},
			"bgName": "bg_main_twilight"
		},
		{
			"side": 2,
			"dir": 1,
			"typewriter": {
				"speed": 0.05,
				"speedUp": 0.01
			},
			"say": "The reason should have been obvious...",
			"painting": {
				"alpha": 0.3,
				"time": 1
			},
			"bgName": "bg_main_twilight"
		},
		{
			"actor": 206050,
			"side": 2,
			"dir": 1,
			"nameColor": "#a9f548",
			"expression": 3,
			"say": "You want me to continue as your secretary? I\u2019d be happy to, of course, but why?",
			"typewriter": {
				"speed": 0.05,
				"speedUp": 0.01
			},
			"action": [
