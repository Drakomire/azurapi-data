			},
			"painting": {
				"alpha": 0.3,
				"time": 1
			},
			"bgName": "bg_main_twilight"
		},
		{
			"actor": 206050,
			"side": 2,
			"dir": 1,
			"nameColor": "#a9f548",
			"expression": 2,
			"say": "I had to subject you to a little unladylike teasing.",
			"typewriter": {
				"speed": 0.05,
				"speedUp": 0.01
			},
			"painting": {
				"alpha": 0.3,
				"time": 1
			},
