			"dir": 1,
			"typewriter": {
				"speed": 0.05,
				"speedUp": 0.01
			},
			"say": "After leaving the Sakura dorms, I told Chaser to come by later as she prepared to return to the Royal Navy.",
			"painting": {
				"alpha": 0.3,
				"time": 1
			},
			"bgName": "bg_main_twilight"
