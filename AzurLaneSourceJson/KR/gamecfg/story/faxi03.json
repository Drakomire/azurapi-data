			"bgm": "level03",
			"actorShadow": true,
			"say": "\uc654\uad6c\ub098\u2026 \ub85c\uc5f4 \ub124\uc774\ube44 \uc5ec\ub7ec\ubd84."
		},
		{
			"actor": 204030,
			"side": 2,
			"dir": -1,
			"typewriter": {
				"speed": 0.05,
				"speedUp": 0.01
			},
			"painting": {
				"alpha": 0.3,
				"time": 1
			},
			"say": "\ud3c9\uc548\ud558\uc2e0\uc9c0\uc694. \ub85c\uc5f4 \ub124\uc774\ube44 H\ud568\ub300 \uae30\ud568 \ud6c4\ub4dc, \uc778\uc0ac\ub4dc\ub9bd\ub2c8\ub2e4."
		},
		{
			"typewriter": {
				"speed": 0.05,
				"speedUp": 0.01
			},
			"say": "\uc637\uc790\ub77d\uc744 \ud55c \uc190\uc73c\ub85c \uc0b4\uc9dd \ub4e4\uc5b4 \uc62c\ub824, \uc0c1\ub300\ub97c \ud5a5\ud574 \uac00\ubccd\uac8c \uace0\uac1c \uc219\uc778\ub2e4\u2026\u2026 \uc6b0\uc544\ud558\uace0 \uaca9\uc2dd \uc788\ub294 \ub85c\uc5f4 \ub124\uc774\ube44\uc2dd \uc608\uc808\uc774\uc5c8\ub2e4.",
			"side": 2,
			"dir": 1
		},
		{
			"actor": 904010,
			"side": 2,
			"dir": 1,
			"typewriter": {
				"speed": 0.05,
				"speedUp": 0.01
			},
			"painting": {
				"alpha": 0.3,
				"time": 1
			},
			"say": "\uc21c\uc591\uc804\ud568 \ub42d\ucf00\ub974\ud06c, \uc778\uc0ac\ub4dc\ub9bd\ub2c8\ub2e4. \uc5ec\ub7ec\ubd84\uaed8 \uc704\ub300\ud55c \uc544\ubc84\uc9c0\uc640 \uc131\ub839\uc758 \ucd95\ubcf5\uc774 \uc788\uae30\ub97c."
		},
		{
			"typewriter": {
				"speed": 0.05,
				"speedUp": 0.01
			},
			"say": "\ube44\uc2dc\uc544 \uc18c\uc18d\uc758 \uc21c\uc591\uc804\ud568\u2026\u2026 \ub2a0\ub984\ud55c \uae30\uc0ac \ub42d\ucf00\ub974\ud06c\ub294 \uc624\ub978\uc190\uc73c\ub85c \uac00\uc2b4\uc5d0 \uc2e0\uc758 \ud45c\uc2dd\uc744 \uadf8\ub9ac\uace0, \ud6c4\ub4dc\ub97c \ud5a5\ud574 \uac00\ubccd\uac8c \uc778\uc0ac\ud588\ub2e4.",
			"side": 2,
			"dir": 1
		},
		{
			"actor": 204030,
			"side": 2,
			"dir": -1,
			"typewriter": {
				"speed": 0.05,
				"speedUp": 0.01
			},
			"painting": {
				"alpha": 0.3,
				"time": 1
			},
			"say": "\uc800\ud76c\uac00 \uc5ec\uae30\uc5d0 \ucc3e\uc544\ubd4c \uc774\uc720\ub294 \uc54c\uace0 \uacc4\uc2dc\ub9ac\ub77c \uc0dd\uac01\ud569\ub2c8\ub2e4\ub9cc\u2026\u2026 \uba54\ud0c8 \ube14\ub7ec\ub4dc\uc5d0 \ub9de\uc11c \ub85c\uc5f4 \ub124\uc774\ube44\uc758 \uc791\uc804 \ud589\ub3d9\uc5d0 \ud611\uc870\ud574\uc8fc\uc2dc\uae38 \ubd80\ud0c1\ub4dc\ub9bd\ub2c8\ub2e4."
		},
		{
			"actor": 904010,
			"side": 1,
			"say": "\u2026\uc678\ub78c\ub41c \ub9d0\uc500\uc774\uc624\ub098, \ud638\uad50\uae30\uc0ac\ub2e8\uc740 \ud0c0\uad6d\uc758 \uc9c0\ub3c4\ub97c \ubc1b\uc744 \uc758\ud5a5\uc740 \uc5c6\uc2b5\ub2c8\ub2e4.",
			"typewriter": {
				"speed": 0.05,
				"speedUp": 0.01
			},
			"dir": 1,
			"painting": {
				"alpha": 0.3,
				"time": 1
			},
			"paintingFadeOut": {
				"time": 0.5,
				"side": 0
			}
		},
		{
			"actor": 204030,
			"side": 0,
			"dir": -1,
			"typewriter": {
				"speed": 0.05,
				"speedUp": 0.01
			},
			"painting": {
				"alpha": 0.3,
				"time": 1
			},
			"say": "\uadf8\ub7fc, \uc800\ud76c \uc81c\uc548\uc744 \uc81c\uc2dc\ud558\uaca0\uc2b5\ub2c8\ub2e4."
		},
		{
			"actor": 204030,
			"side": 0,
			"dir": -1,
			"typewriter": {
				"speed": 0.05,
				"speedUp": 0.01
			},
			"painting": {
				"alpha": 0.3,
				"time": 1
			},
			"say": "\uccab\uc9f8, \uc2e0\uc18d\ud788 \ud56d\uad6c\uc5d0\uc11c \ub098\uc640 \ub85c\uc5f4 \ub124\uc774\ube44\uc640 \ud569\ub958\ud558\uc5ec, \uba54\ud0c8 \ube14\ub7ec\ub4dc\uc640 \ub808\ub4dc \uc561\uc2dc\uc2a4\uc5d0 \ub9de\uc11c\ub294 \uc791\uc804 \ud589\ub3d9\uc5d0 \ub098\uc124 \uac83."
		},
		{
			"actor": 204030,
			"side": 0,
			"dir": -1,
			"typewriter": {
				"speed": 0.05,
				"speedUp": 0.01
			},
			"painting": {
				"alpha": 0.3,
				"time": 1
			},
			"say": "\ub458\uc9f8, \ubb34\uc7a5\uc744 \ud574\uc81c\ud558\uace0, \ub85c\uc5f4 \ub124\uc774\ube44 \uc138\ub825 \ud558\uc758 \ud56d\uad6c\ub85c \uc774\ub3d9\ud560 \uac83\u2026\u2026 \uc774\ub3d9 \uc911\uc758 \uc548\uc804\uc740 \ub85c\uc5f4 \ub124\uc774\ube44\uac00 \ubcf4\uc7a5\ud55c\ub2e4."
		},
		{
			"actor": 204030,
			"side": 0,
			"dir": -1,
			"typewriter": {
				"speed": 0.05,
				"speedUp": 0.01
			},
			"painting": {
				"alpha": 0.3,
				"time": 1
			},
			"say": "\uc14b\uc9f8, \ub85c\uc5f4 \ub124\uc774\ube44\uc758 \uac10\ub3c5\uc5d0 \ub530\ub77c, \uc720\ub2c8\uc628\uc758 \uc9c0\ud718 \ud558\uc5d0 \ub4e4\uc5b4\uac00 \ubb34\uc7a5\uc744 \ud574\uc81c\ud560 \uac83."
		},
		{
			"actor": 204030,
			"side": 0,
			"dir": -1,
			"typewriter": {
				"speed": 0.05,
				"speedUp": 0.01
			},
			"painting": {
				"alpha": 0.3,
				"time": 1
			},
			"say": "\ub137\uc9f8, \uc9c0\uae08 \uc774 \uc790\ub9ac\uc5d0\uc11c \uc2a4\uc2a4\ub85c \uce68\ubab0\ud560 \uac83.\u2026\u2026 \uc774\uac83\ub9cc\uc740 \ubcf4\uace0 \uc2f6\uc9c0 \uc54a\ub124\uc694."
		},
		{
			"actor": 904010,
			"side": 1,
			"dir": 1,
			"typewriter": {
				"speed": 0.05,
				"speedUp": 0.01
			},
			"painting": {
				"alpha": 0.3,
				"time": 1
			},
			"say": "\u2026\u2026\uadf8\uac8c \uc5ec\ub7ec\ubd84\uc758 \ucd5c\uc885 \ud1b5\ubcf4\ub77c\uace0 \uc774\ud574\ud558\uba74 \ub420\uae4c\uc694? .\u2026\ud639\uc2dc \ubaa8\ub4e0 \uc81c\uc548\uc744 \uc218\ub77d\ud560 \uc218 \uc5c6\ub2e4\uace0 \ud55c\ub2e4\uba74?"
		},
		{
			"actor": 204030,
			"side": 0,
			"dir": -1,
			"typewriter": {
				"speed": 0.05,
				"speedUp": 0.01
			},
			"painting": {
				"alpha": 0.3,
				"time": 1
			},
			"say": "\ubcf8\ubd80\uc758 \uc9c0\uc2dc\uc5d0 \ub530\ub77c, '\ubb34\ub825\uc744 \uc368\uc11c\ub77c\ub3c4 \uc801 \ud568\ub300\uc758 \ubb34\ub825\ud654\ub97c \uc2dc\ud589\ud55c\ub2e4'. \u2026\u2026\uadf8\ub7f4 \uc218\ubc16\uc5d0 \uc5c6\uaca0\uad70\uc694."
		},
		{
			"actor": 904010,
			"side": 1,
			"dir": 1,
			"typewriter": {
				"speed": 0.05,
				"speedUp": 0.01
			},
			"painting": {
				"alpha": 0.3,
				"time": 1
			},
			"say": "\u2026\u2026"
		},
		{
			"actor": 204030,
			"side": 0,
			"dir": -1,
			"typewriter": {
				"speed": 0.05,
				"speedUp": 0.01
			},
			"painting": {
				"alpha": 0.3,
				"time": 1
			},
			"say": "\uc544\uc774\ub9ac\uc2a4\ub294 \uc5b4\uca54 \uc218 \uc5c6\uc2b5\ub2c8\ub2e4\ub9cc\u2026\u2026 \uc804\uc0ac\ub77c\uba74 \uc790\uc2e0\uc758 \uc870\uad6d\uc744 \uc704\ud574 \ud798\uc744 \uc4f0\ub294 \uac83\uc774 \uc219\uc6d0\uc77c \uac70\uc5d0\uc694. \ub2e4\uc2dc \ud55c \ubc88\ub9cc \uc0dd\uac01\ud574\ubcf4\uc2dc\uae30 \ubc14\ub78d\ub2c8\ub2e4. \uc800\ud76c \ub85c\uc5f4 \ub124\uc774\ube44\uc640 \ud568\uaed8 \uc2f8\uc6b0\uc9c0 \uc54a\uc73c\uc2dc\uaca0\uc5b4\uc694?"
		},
		{
			"actor": 904010,
			"side": 1,
			"dir": 1,
			"typewriter": {
				"speed": 0.05,
				"speedUp": 0.01
			},
			"painting": {
				"alpha": 0.3,
				"time": 1
			},
			"say": "\uac70\uc808\ud558\uaca0\uc5b4."
		},
		{
			"typewriter": {
				"speed": 0.05,
				"speedUp": 0.01
			},
			"say": "\ub42d\ucf00\ub974\ud06c\ub294 \uc758\uc7a5\uc5d0 \uc7a5\ucc29\ub418\uc5b4 \uc788\ub358 \uc0ac\ubca8\uc744 \ud55c \uc190\uc73c\ub85c \ubf51\uc544 \ub4e4\uc5c8\ub2e4.",
			"side": 2,
			"dir": 1
		},
		{
			"actor": 904010,
			"side": 2,
			"dir": 1,
			"typewriter": {
				"speed": 0.05,
				"speedUp": 0.01
			},
			"painting": {
				"alpha": 0.3,
				"time": 1
			},
			"say": "\ucd94\uae30\uacbd\uacfc \uc2e0\uc559\uc5d0 \ucda9\uc131\uc744 \ub2e4\ud558\ub294 \uac83\uc774 \ud638\uad50\uae30\uc0ac\ub2e8. \uc678\uc9c0\uc778\uc758 \uba85\ub839\uc744 \ub530\ub974\ub294 \uac74 \uc788\uc744 \uc218 \uc5c6\uc5b4."
		},
		{
			"actor": 904010,
			"side": 2,
			"dir": 1,
			"typewriter": {
				"speed": 0.05,
				"speedUp": 0.01
			},
			"painting": {
				"alpha": 0.3,
				"time": 1
			},
			"say": "\uacf5\uaca9\ud574\uc624\uc9c0 \uc54a\ub294\ub2e4\uba74, \uba54\ud0c8 \ube14\ub7ec\ub4dc\u2026 \uadf8\ub9ac\uace0 \uc81c\uad6d \uce21\uc5d0 \uac00\uc138\ud558\uc9c0\ub294 \uc54a\uc744 \uac83\uc744 \uc57d\uc18d\ud558\uc9c0."
		},
		{
			"actor": 204030,
			"side": 0,
			"say": "\uadf8\ub807\ub2e4\uba74, \uadf8 \uc99d\uac70\ub85c \ubb34\uc7a5\uc744 \ud574\uc81c\ud574\uc8fc\uc2dc\uc9c0 \uc54a\uaca0\uc5b4\uc694?",
			"typewriter": {
				"speed": 0.05,
				"speedUp": 0.01
			},
			"dir": -1,
			"painting": {
				"alpha": 0.3,
				"time": 1
			},
			"paintingFadeOut": {
				"time": 0.5,
				"side": 1
			}
		},
		{
			"actor": 904010,
			"side": 1,
			"dir": 1,
			"typewriter": {
				"speed": 0.05,
				"speedUp": 0.01
			},
			"painting": {
				"alpha": 0.3,
				"time": 1
			},
			"say": "\u2026\u2026\ub9f9\uc6b0\ub77c\uba74\uc11c, \uc774 \uc815\ub3c4 \uc57d\uc18d\uc870\ucc28 \ubbff\uc5b4\uc8fc\uc9c0 \ubabb\ud558\ub294 \uac70\ub124."
		},
		{
			"actor": 204030,
			"side": 0,
			"dir": -1,
			"typewriter": {
				"speed": 0.05,
				"speedUp": 0.01
			},
			"painting": {
				"alpha": 0.3,
				"time": 1
			},
			"say": "\uadf8\ub54c\u2026\u2026 \uc774 \ub300\ub959\uc5d0\uc11c \ucca0\uc218\ud588\ub358 \ub0a0\ubd80\ud130, \ub2f9\uc2e0\ub4e4\uacfc \uc5f0\ub77d\uc774 \ub04a\uacbc\ub2e4\uace0 \ubcf8\ubd80\uac00 \ub9d0\ud588\uc2b5\ub2c8\ub2e4."
		},
		{
			"actor": 204030,
			"side": 0,
			"dir": -1,
			"typewriter": {
				"speed": 0.05,
				"speedUp": 0.01
			},
			"painting": {
				"alpha": 0.3,
				"time": 1
			},
			"say": "\uc774 \ud574\uc5ed\uc5d0 \uc788\ub294 \uc138\uc774\ub80c\uc758 \ub3d9\ud5a5\uc744 \ubd10\ub3c4, \uc9c0\uae08 \ub2f9\uc2e0\uc758 \ub9d0\uc744 \uc561\uba74 \uadf8\ub300\ub85c \ubc1b\uc544\ub4e4\uc774\ub294 \uac74 \ud798\ub4e4\uc5b4\ubcf4\uc785\ub2c8\ub2e4. \u2026\u2026\ubd80\ub514, \uc800\ud76c \uc785\uc7a5\uc744 \uc774\ud574\ud574\uc8fc\uc2dc\uae30 \ubc14\ub798\uc694."
		},
		{
			"actor": 904010,
			"side": 1,
			"dir": 1,
			"typewriter": {
				"speed": 0.05,
				"speedUp": 0.01
			},
			"painting": {
				"alpha": 0.3,
				"time": 1
			},
			"say": "\uc591\uc0b0\ud615 \uc138\uc774\ub80c\uc774 \uc788\ub294 \uac74 \uba54\ud0c8 \ube14\ub7ec\ub4dc \ud0d3\uc774\uc57c. \uc6b0\ub9ac\ub4e4\uc758 \ub3d9\ud5a5\uc744 \uac10\uc2dc\ud558\ub294 \uc774\uc720\ub3c4 \ud3ec\ud568\ud574\uc11c \ub9d0\uc774\uc9c0."
		},
		{
			"actor": 904010,
			"side": 1,
			"dir": 1,
			"typewriter": {
				"speed": 0.05,
				"speedUp": 0.01
			},
			"painting": {
				"alpha": 0.3,
				"time": 1
			},
			"say": "\uadf8\uac78 \uccad\uc18c\ud574\uc900 \ub2f9\uc2e0\ub4e4\uc5d0\uac90 \uc624\ud788\ub824 \uac10\uc0ac\ud558\uace0 \uc2f6\ub124."
		},
		{
			"actor": 204030,
			"side": 0,
			"dir": -1,
			"typewriter": {
				"speed": 0.05,
				"speedUp": 0.01
			},
			"painting": {
				"alpha": 0.3,
				"time": 1
			},
			"say": "\u2026\u2026\u2026\u2026\uc774\uc81c \uac08 \uc2dc\uac04\uc774 \ub410\ub124\uc694. \uc5ec\uae30\uc11c\ub294\u2026\u2026 \ubcf8\ubd80\ub85c\uc11c, '\ube44\uc2dc\uc544 \uc131\uc88c\uc758 \uc815\uc2dd \ub2f5\ubcc0'\uc744 \uae30\ub300\ud558\uace0 \uc788\uaca0\uc2b5\ub2c8\ub2e4."
		},
		{
			"actor": 904010,
			"side": 1,
			"dir": 1,
			"typewriter": {
				"speed": 0.05,
				"speedUp": 0.01
			},
			"painting": {
				"alpha": 0.3,
				"time": 1
			},
			"say": "'\ub85c\uc5f4 \ub124\uc774\ube44 \ud568\ub300\ub294 \uc2e0\uc6a9\ud560 \uc218 \uc5c6\ub2e4. \ub9c8\uc9c0\ub9c9\uae4c\uc9c0 \uc2f8\uc6b0\uaca0\ub2e4.' \u2026\u2026\uc774\uac8c \ub2f5\ubcc0\uc774\uc57c."
		},
