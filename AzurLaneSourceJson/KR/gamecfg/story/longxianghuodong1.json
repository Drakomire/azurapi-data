			},
			"painting": {
				"alpha": 0.3,
				"time": 1
			},
			"say": "\uc660\uc9c0 \uc9c8\ub838\uc5b4~ \uc5d4\ud130\ud504\ub77c\uc774\uc988, \uc880\ub9cc \ub354 \uc788\ub2e4\uac00 \ubbf8\ub2c8\uc560\ud3f4\ub9ac\uc2a4\uac00 \uc788\ub294 \ub370\uc11c \ubc14\ubca0\ud050 \ud560\uae4c?"
		},
		{
			"say": "\uc0c8\ub7ec\ud1a0\uac00, \uae34\uc7a5\uc744 \ub2a6\ucd94\uc9c0 \ub9c8. \uc27d\uac8c \ub05d\ub0bc \uc218 \uc788\ub294 \uc784\ubb34\uac00 \uc544\ub2c8\ub77c\uace0.",
			"actor": 107060,
			"side": 0,
			"dir": -1,
			"nameColor": "#92fc63",
			"typewriter": {
				"speed": 0.05,
				"speedUp": 0.01
			},
			"painting": {
				"alpha": 0.3,
				"time": 1
			},
			"paintingFadeOut": {
				"time": 0.5,
				"side": 1
			}
		},
		{
			"actor": 107030,
			"side": 1,
			"dir": 1,
			"nameColor": "#92fc63",
			"typewriter": {
				"speed": 0.05,
				"speedUp": 0.01
			},
			"painting": {
				"alpha": 0.3,
				"time": 1
			},
			"say": "\uc54c\uace0 \uc788\ub2e4\uad6c~ \uadf8\ub0e5 \ud574\ubcf8 \ub9d0\uc774\uc57c\u266a \uc5ec\uae30 \ube44\ud589\uc7a5\uc740 \uaf64 \uc911\uc694\ud558\uc796\uc544? \uc544\ubb34\ub9ac \ub098\ub77c\uace0 \ud574\ub3c4 \uadf8 \uc815\ub3c4\ub294 \uc54c\uace0 \uc788\uc73c\ub2c8\uae4c \ub9d0\uc57c."
		},
		{
			"actor": 107060,
			"side": 0,
			"dir": -1,
			"nameColor": "#92fc63",
			"typewriter": {
				"speed": 0.05,
				"speedUp": 0.01
			},
			"painting": {
				"alpha": 0.3,
				"time": 1
			},
			"say": "\uc0c1\ub300\ud3b8\uc774 \uc21c\uc21c\ud788 \uc810\ub839\ub2f9\ud574 \uc904 \ub9ac \uc5c6\uc5b4. \uc815\ucc30\uae30\ub3c4 \ubc8c\uc368 \uc0ac\ucfe0\ub77c \uc5e0\ud30c\uc774\uc5b4\uc758 \ud568\ub300\ub97c \ubc1c\uacac\ud588\ub2e4\ub294 \ubaa8\uc591\uc774\ub2e4."
		},
		{
			"actor": 107060,
			"side": 0,
			"dir": -1,
			"nameColor": "#92fc63",
			"typewriter": {
				"speed": 0.05,
				"speedUp": 0.01
			},
			"painting": {
				"alpha": 0.3,
				"time": 1
			},
			"say": "\uac8c\ub2e4\uac00 \uc640\uc2a4\ud504\ub294 \ubcf4\uae09\uc744 \uc704\ud574 \uba3c\uc800 \ub0a8\ucabd\uc73c\ub85c \ub0b4\ub824\uac14\uc73c\ub2c8, \uc774\ubc88\uc5d0\ub294 \uc815\ub9d0 \uc6b0\ub9ac\ub9cc\uc73c\ub85c \uc5b4\ub5bb\uac8c \ud574\ubcfc \uc218\ubc16\uc5d0."
		},
		{
			"actor": 107060,
			"side": 0,
			"dir": -1,
			"nameColor": "#92fc63",
			"typewriter": {
				"speed": 0.05,
				"speedUp": 0.01
			},
			"painting": {
				"alpha": 0.3,
				"time": 1
			},
			"say": "\uc0dd\uac01\ud574\ubcf4\ub2c8, \uc0ac\ubcf4\uc12c\uc758 \uc804\ud22c\uc5d0\uc11c \uc190\ud574\uac00 \ud06c\uc9c4 \uc54a\uc558\uc9c0\ub9cc\u2026\u2026\ud639\uc2dc, \ub0b4\uac00 \uadf8 \ub54c \uadf8\uacf3\uc5d0 \uc788\uc5c8\ub2e4\uba74\u2026\u2026"
		},
		{
			"actor": 107030,
			"side": 1,
			"dir": 1,
			"nameColor": "#92fc63",
			"typewriter": {
				"speed": 0.05,
				"speedUp": 0.01
			},
			"painting": {
				"alpha": 0.3,
				"time": 1
			},
			"say": "\uc5d4\ud130\ud504\ub77c\uc774\uc988\uc758 \uc798\ubabb\uc774 \uc544\ub2c8\uc57c. \uc0c1\ubd80\uc758 \uba85\ub839\uc774\ub2c8\uae4c \ubcc4\uc218 \uc5c6\uc796\uc544?"
		},
		{
			"actor": 107030,
			"side": 1,
			"dir": 1,
			"nameColor": "#92fc63",
			"typewriter": {
				"speed": 0.05,
				"speedUp": 0.01
			},
			"painting": {
				"alpha": 0.3,
				"time": 1
			},
			"say": "\uc544\uc2a4\ud1a0\ub9ac\uc544 \uc77c\ud589\uc774 \uc911\uc0c1\uc744 \uc785\uc740 \uac74 \ubd84\ud558\uc9c0\ub9cc, \uadf8\ub807\uac8c \uce68\uc6b8\ud574 \uc788\uc5b4\ub3c4 \ubc14\ub00c\ub294 \uac74 \uc544\ubb34\uac83\ub3c4 \uc5c6\uc796\uc544? \uace0\ubbfc\uc740 \uc77c\ub2e8 \uc784\ubb34\uac00 \ub05d\ub09c \ub2e4\uc74c\uc5d0 \ud558\uc790\uad6c!"
		},
		{
			"actor": 107060,
			"side": 0,
			"dir": -1,
			"nameColor": "#92fc63",
			"typewriter": {
				"speed": 0.05,
				"speedUp": 0.01
			},
			"painting": {
				"alpha": 0.3,
				"time": 1
			},
			"say": "\uc54c\uace0 \uc788\uc5b4. \uadf8\ub807\uc9c0\ub9cc\u2026\u2026"
		},
		{
			"actor": 107030,
			"side": 1,
			"dir": 1,
			"nameColor": "#92fc63",
			"typewriter": {
				"speed": 0.05,
				"speedUp": 0.01
			},
			"painting": {
				"alpha": 0.3,
				"time": 1
			},
			"say": "\uc790 \uadf8\ub7fc \uac70\uae30\uae4c\uc9c0! \uc5d4\ud130\ud504\ub77c\uc774\uc988, \uc694\uc998 \uc5f4\uc2ec\ud788 \ud574\uc8fc\ub294 \uac74 \ub108\uc5b4~\ubb34 \uc798 \uc54c\uace0 \uc788\uc9c0\ub9cc!"
		},
		{
			"actor": 107030,
			"side": 1,
			"dir": 1,
			"nameColor": "#92fc63",
			"typewriter": {
				"speed": 0.05,
				"speedUp": 0.01
			},
			"painting": {
				"alpha": 0.3,
				"time": 1
			},
			"say": "\uc791\uc804 \uc911\uc5d0 \ub0c9\uc815\ud574\uc9c0\uc9c0 \uc54a\uc73c\uba74 \uc2e4\uc218\ud558\uae30 \ub9c8\ub828\uc774\uc796\uc544? \uadf8\u00b7\ub7ec\u00b7\ub2c8\u00b7\uae4c! \uc774\ub7f4 \ub54c\uc77c\uc218\ub85d \ub354 \ub9b4\ub799\uc2a4, \ub9b4\ub799\uc2a4~"
		},
		{
			"actor": 107060,
			"side": 0,
			"dir": -1,
			"nameColor": "#92fc63",
			"typewriter": {
				"speed": 0.05,
				"speedUp": 0.01
			},
			"painting": {
				"alpha": 0.3,
				"time": 1
			},
			"say": "\u2026\u2026\uc54c\uaca0\ub2e4. \uadf8\ub807\uac8c \ud558\uc9c0."
		},
		{
			"actor": 107060,
			"side": 0,
			"dir": -1,
			"nameColor": "#92fc63",
			"typewriter": {
				"speed": 0.05,
				"speedUp": 0.01
			},
			"painting": {
				"alpha": 0.3,
				"time": 1
			},
			"say": "\ubbf8\uc548\ud558\uad70. \ub108\ub3c4 \ubd84\uba85 \ub809\uc2f1\ud134\uc744 \uac71\uc815\ud558\uace0 \uc788\uc744 \ud150\ub370, \ub098\uae4c\uc9c0 \uc774\ub798 \ubc84\ub9ac\ub2e4\ub2c8\u2026\u2026"
		},
		{
			"actor": 107030,
			"side": 1,
			"dir": 1,
			"nameColor": "#92fc63",
			"typewriter": {
				"speed": 0.05,
				"speedUp": 0.01
			},
			"painting": {
				"alpha": 0.3,
				"time": 1
			},
			"say": "\uad1c\ucc2e\uc544 \uad1c\ucc2e\uc544! \uc0c8\ub7ec\ud1a0\uac00 \ud55c\ud14c \uc774\ub7f0 \uac74 \uc544\ubb34 \uc77c\ub3c4 \uc544\ub2c8\ub2c8\uae4c!"
		},
		{
			"actor": 107030,
			"side": 1,
			"dir": 1,
			"nameColor": "#92fc63",
			"typewriter": {
				"speed": 0.05,
				"speedUp": 0.01
			},
			"painting": {
				"alpha": 0.3,
				"time": 1
			},
			"say": "\uac8c\ub2e4\uac00, \uc5b8\ub2c8\ub3c4 \ubd84\uba85 \uc9c0\uae08 \uc774\ub300\ub85c\uc758 \ub0b4\uac00 \uc88b\ub2e4\uace0 \uc598\uae30\ud574\uc92c\uc744 \uac70\uc57c!"
		},
		{
			"actor": 107060,
			"side": 0,
			"dir": -1,
			"nameColor": "#92fc63",
			"typewriter": {
				"speed": 0.05,
				"speedUp": 0.01
			},
			"painting": {
				"alpha": 0.3,
				"time": 1
			},
			"say": "\ub098\ub3c4 \uc0c8\ub7ec\ud1a0\uac00\ub97c \ubc30\uc6cc\uc57c\uaca0\uad70. \ub3d9\ub8cc\ub4e4\uc758 \ube5a\uc740 \ud655\uc2e4\ud788 \ub3cc\ub824\uc8fc\ub3c4\ub85d \ud558\uc790\uace0."
		},
		{
			"actor": 107030,
			"side": 1,
			"dir": 1,
			"nameColor": "#92fc63",
			"typewriter": {
				"speed": 0.05,
				"speedUp": 0.01
			},
			"painting": {
				"alpha": 0.3,
				"time": 1
			},
			"say": "\uadf8\ub7f0 \ud0dc\ub3c4 \uc88b\uc544! \uadf8\ub798\uc11c, \ubc14\ubca0\ud050 \uac08 \uac70\uc57c? \uc548 \uac08 \uac70\uc57c?"
		},
		{
			"actor": 107060,
			"side": 0,
			"dir": -1,
			"nameColor": "#92fc63",
			"typewriter": {
				"speed": 0.05,
				"speedUp": 0.01
			},
			"painting": {
				"alpha": 0.3,
				"time": 1
			},
			"say": "\uadf8\ub807\uc9c0\u2026\u2026\ud558\ud558, \uadf8\ub7fc \ub098\ub3c4 \ud568\uaed8 \uc2e4\ub840\ud558\ub3c4\ub85d \ud560\uae4c."
		}
	],
