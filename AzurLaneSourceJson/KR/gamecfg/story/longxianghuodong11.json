			"nameColor": "#92fc63",
			"typewriter": {
				"speed": 0.05,
				"speedUp": 0.01
			},
			"painting": {
				"alpha": 0.3,
				"time": 1
			},
			"say": "\uacfc\uc5f0 \uadf8\ub807\uad70. \ud55c\ucc3d \uc2f8\uc6b0\uace0 \uc788\uc744 \ub54c \uac70\uae30\uae4c\uc9c0 \uc0dd\uac01\ud588\ub2e4 \uc774\uac74\uac00."
