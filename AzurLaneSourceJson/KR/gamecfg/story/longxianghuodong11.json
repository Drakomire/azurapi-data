			"painting": {
				"alpha": 0.3,
				"time": 1
			},
			"say": "\u2026\u2026\uc218\uc1a1\ud568\uc120\uc744 \ud638\uc704\ud558\ub294 \ub2e4\ub978 \ud568\ub300\uac00 \uc788\ub2e4\ub294 \uac74\uac00."
