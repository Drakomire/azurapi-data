			"typewriter": {
				"speed": 0.05,
				"speedUp": 0.01
			},
			"painting": {
				"alpha": 0.3,
				"time": 1
			},
			"say": "\uba54\ud0c8 \ube14\ub7ec\ub4dc\uc5d0\ub294 \uc911\ud615\ud568\uc774 \uc801\uc73c\ub2c8\uae4c, \uc5ec\uae30\uc5d4\u2026\u2026"
