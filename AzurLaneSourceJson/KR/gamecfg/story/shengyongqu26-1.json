				"speed": 0.05,
				"speedUp": 0.01
			},
			"painting": {
				"alpha": 0.3,
				"time": 1
			},
			"say": "\u2026\u2026\uc54c\uc81c\ub9ac\uac00 \uacb0\uc804\uc744 \uc704\ud574\uc11c \uc2e4\ub825\uc744 \uc228\uae30\uace0 \uc788\ub2e4\uace0 \uc0dd\uac01\ud588\ub294\ub370, \uc9c0\uae08\uae4c\uc9c0\ub3c4 \uc9c0\uc6d0\uad70\uc774 \uc5c6\ub294 \uac83\uc744 \ubcf4\uba74 \uadf8 \uc815\ubcf4\uac00 \uc798\ubabb\ub41c \uac83\uc77c \uc218\ub3c4 \uc788\uaca0\uad70\uc694."
