			"dir": 1,
			"nameColor": "#a9f548",
			"typewriter": {
				"speed": 0.05,
				"speedUp": 0.01
			},
			"painting": {
				"alpha": 0.3,
				"time": 1
			},
			"say": "\uacb0\uc804\uc758 \uc2dc\uac04\uc774 \ub4dc\ub514\uc5b4 \ub2e4\uac00\uc654\uad70\uc694. \ud558\ub098, \ub458, \uc14b\u2026\u2026"
