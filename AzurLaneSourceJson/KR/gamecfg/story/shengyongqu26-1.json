			"painting": {
				"alpha": 0.3,
				"time": 1
			},
			"say": "\uae30\uc5b5 \uc18d\uc758 \uc131\ub2f9\uc740 \ubd84\uba85\ud788 \uc544\uc774\ub9ac\uc2a4 \uad50\uad6d\uc758 \uc0c1\uc9d5\uc0c9\uc778 \ud478\ub978\uc0c9\uc774\uc5c8\ub294\ub370, \uc774\ub807\uac8c \ube68\uac04\uc0c9\uc73c\ub85c \ubcc0\ud574\ubc84\ub9ac\ub2e4\ub2c8\u2026\u2026"
