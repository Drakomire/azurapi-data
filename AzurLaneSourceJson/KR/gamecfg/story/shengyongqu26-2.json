			},
			"painting": {
				"alpha": 0.3,
				"time": 1
			},
			"say": "\uc54c\uc81c\ub9ac, \ubcf4\ud074\ub7ad, \ub77c \uac08\ub9ac\uc18c\ub2c8\uc5d0\ub974. \uc774\ubc88 \uc804\ud22c\uc758 \uc2b9\ud328\ub294 \uc774\ubbf8 \uba85\ud655\ud569\ub2c8\ub2e4."
		},
		{
			"say": "\ud6cc\ub96d\ud55c \ubc29\uc5b4\uc600\uc9c0\ub9cc, \uc804\uc220\ub85c \uba54\uc6b8 \uc218 \uc5c6\ub294 \uc2e4\ub825\uc758 \ucc28\uc774\ub77c\ub294 \uac83\uc774 \uc874\uc7ac\ud574\uc694.",
			"actor": 805010,
			"side": 2,
			"dir": 1,
			"nameColor": "#a9f548",
			"typewriter": {
				"speed": 0.05,
				"speedUp": 0.01
			},
			"painting": {
				"alpha": 0.3,
				"time": 1
			},
			"expression": 2
		},
		{
			"say": "\uc774\ubbf8 '\uccb4\ud06c \uba54\uc774\ud2b8' \uc0c1\ud0dc\ub77c\ub294 \uac83\uc740 \uc5ec\ub7ec\ubd84\ub3c4 \uc798 \uc54c\uace0 \uc788\uc744 \uac83\uc785\ub2c8\ub2e4. \uacc4\uc18d \uc2f8\uc6b4\ub2e4 \ud574\ub3c4 \uacb0\uacfc\ub294 \ubc14\ub00c\uc9c0 \uc54a\uc544\uc694.",
			"actor": 805010,
			"side": 2,
			"dir": 1,
			"nameColor": "#a9f548",
			"typewriter": {
				"speed": 0.05,
				"speedUp": 0.01
			},
			"painting": {
				"alpha": 0.3,
				"time": 1
			},
			"expression": 4
		},
		{
			"say": "\uc11c\ub85c \uc774\ub807\uac8c\uae4c\uc9c0 \ub0a0\uc744 \uc138\uc6b8 \ud544\uc694\ub294 \uc5c6\uc2b5\ub2c8\ub2e4. \uc778\uac04\uc740 \uae30\ub2e4\ub9ac\ub2e4 \ubcf4\uba74 \uc5b8\uc820\uac00 \uad6c\uc6d0\ubc1b\uc744 \uc218 \uc788\uc9c0\uc694. \ud558\uc9c0\ub9cc \uad6d\uac00\ub294 \uadf8\ub807\uc9c0 \uc54a\uc73c\ub2c8 \ud604\uc7ac\ub97c \uad6c\ud574\uc57c \ud569\ub2c8\ub2e4!",
			"actor": 805010,
			"side": 2,
			"dir": 1,
			"nameColor": "#a9f548",
			"typewriter": {
				"speed": 0.05,
				"speedUp": 0.01
			},
			"painting": {
				"alpha": 0.3,
				"time": 1
			},
			"expression": 2
		},
		{
			"say": "\uc544\uc774\ub9ac\uc2a4 \ub9ac\ube0c\ub808\ub85c \uc624\uc138\uc694. \ucd94\uae30\uacbd \ub9ac\uc290\ub9ac\uc678\uc758 \uc774\ub984\uc73c\ub85c \uc5ec\ub7ec\ubd84\uc5d0\uac8c \uc870\uad6d\uc758 \uc601\uad11\uc744 \ub418\ucc3e\uc744 \uac83\uc774\ub77c\uace0 \ub9f9\uc138\ud569\ub2c8\ub2e4!",
			"actor": 805010,
			"side": 2,
			"dir": 1,
			"nameColor": "#a9f548",
			"typewriter": {
				"speed": 0.05,
				"speedUp": 0.01
			},
			"painting": {
				"alpha": 0.3,
				"time": 1
			},
			"expression": 1
		},
		{
			"say": "\ucc9c\ud558\uc758 \ub9ac\uc290\ub9ac\uc678 \ucd94\uae30\uacbd\uc774 \uc65c \uc774\ub807\uac8c \ub9dd\uc124\uc774\ub294 \uac78\uae4c? \uc774 \ubb38\uc81c\uc5d0 \ub300\ud574\uc11c\ub294 \uc774\ubbf8 \uc774\uc57c\uae30\uac00 \ub05d\ub0ac\ub2e4\uace0 \uc0dd\uac01\ud588\ub294\ub370 \ub9d0\uc774\uc57c.",
			"actor": 903020,
			"side": 2,
			"dir": 1,
			"nameColor": "#ff5c5c",
			"typewriter": {
				"speed": 0.05,
				"speedUp": 0.01
			},
			"painting": {
				"alpha": 0.3,
				"time": 1
			},
			"expression": 1
		},
		{
			"actor": 903020,
			"side": 2,
