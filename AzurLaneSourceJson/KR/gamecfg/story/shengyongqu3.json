			"painting": {
				"alpha": 0.3,
				"time": 1
			},
			"say": "\uadf8\ub7ec\uace0 \ubcf4\ub2c8 \uc65c \uc5ec\uae30\ub97c \ubaa9\ud45c\ub85c \uc0bc\uc740 \uac83\uc77c\uae4c\uc694? \ud48d\uacbd\uc740 \uc88b\uc9c0\ub9cc \uc804\ub7b5\uc801\uc778 \uce21\uba74\uc5d0\uc11c\ub294 \uc804\ud600 \uac00\uce58\uac00 \uc5c6\ub294 \uacf3\uc778\ub370\uc694."
		},
		{
			"actor": 207030,
			"side": 2,
			"dir": 1,
			"nameColor": "#a9f548",
			"typewriter": {
				"speed": 0.05,
				"speedUp": 0.01
			},
			"painting": {
				"alpha": 0.3,
				"time": 1
			},
			"say": "\ube44\uc2dc\uc544 \uc131\uc88c\ub3c4 \uc5ec\ub7ec\ucc28\ub840 \ubcd1\ub825\uc744 \uc774\uacf3\uc73c\ub85c \ud30c\uacac\ud588\uc9c0\uc694. \uadf8\ub54c\ub9c8\ub2e4 \uba54\ud0c8 \ube14\ub7ec\ub4dc\uc758 \uc7a0\uc218\ud568\ub3c4 \ud56d\uc0c1 \uc774\uacf3\uc744 \uc21c\ucc30\ud558\uace0 \uc788\uc5c8\uc5b4\uc694."
		},
		{
			"actor": 207030,
			"side": 2,
			"dir": 1,
			"nameColor": "#a9f548",
			"typewriter": {
				"speed": 0.05,
				"speedUp": 0.01
			},
			"painting": {
				"alpha": 0.3,
				"time": 1
			},
			"say": "\uadf8\ub9ac\uace0 \ub2f9\uc2e0 \uc5ed\uc2dc, \uadf8\ub807\uac8c \uc624\ub79c \uc2dc\uac04 \uc900\ube44\ub97c \ud55c \uc774\ud6c4\uc5d0 \uc544\uc774\ub9ac\uc2a4 \ub9ac\ube0c\ub808\uc758 \uccab \ub300\ud615 \uc791\uc804\uc9c0\ub85c \uc774\uacf3\uc744 \uc120\ud0dd\ud588\uc9c0\uc694."
		},
		{
			"actor": 207030,
			"side": 2,
			"dir": 1,
			"nameColor": "#a9f548",
			"typewriter": {
				"speed": 0.05,
				"speedUp": 0.01
			},
			"painting": {
				"alpha": 0.3,
				"time": 1
			},
			"say": "\uc804\ub7b5\uc801 \uac00\uce58\uac00 \ub192\uc9c0 \uc54a\uc740 \ubaa9\ud45c\uc5d0 \uc774\ub807\uac8c \ud558\ub2e4\ub2c8, \uc774\uc0c1\ud558\ub2e4\ub294 \uc0dd\uac01\uc744 \uc9c0\uc6b8 \uc218 \uc5c6\ub124\uc694."
		},
		{
			"actor": 207030,
			"side": 2,
			"dir": 1,
			"nameColor": "#a9f548",
			"typewriter": {
				"speed": 0.05,
				"speedUp": 0.01
			},
			"painting": {
				"alpha": 0.3,
				"time": 1
			},
			"say": "\uc774\uc81c \uace7 \uad50\uc804\uc774 \uc77c\uc5b4\ub0a0 \uac70\uc608\uc694. \uc5f0\ud569 \uc791\uc804\uc758 \ub9f9\uc6b0\ub85c\uc11c, \uadf8\ub9ac\uace0 \uc81c \uc0ac\uc18c\ud55c \ud638\uae30\uc2ec\uc744 \uc704\ud574\uc11c\ub77c\ub3c4 \uc774\uacf3\uc5d0 \uc544\uc774\ub9ac\uc2a4 \uad50\uad6d\uc758 \ud1b5\uc77c\uc744 \uc704\ud55c \uccab \ubc1c\uac78\uc74c\uc744 \ub0b4\ub51b\uc740 \uc774\uc720\ub97c \uc54c\ub824\uc8fc\uc2e4 \uc218 \uc788\ub098\uc694?"
		},
		{
			"actor": 805010,
			"side": 2,
			"dir": 1,
			"nameColor": "#a9f548",
			"typewriter": {
				"speed": 0.05,
				"speedUp": 0.01
			},
			"painting": {
				"alpha": 0.3,
				"time": 1
			},
			"say": "\uc74c\u2026\u2026 \uc544\uc774\ub9ac\uc2a4 \uad50\uad6d\uc758 '\uc131\ub2f9'\uc5d0 \uad00\ud55c \uc18c\ubb38\uc744 \ub85c\uc5f4 \ub124\uc774\ube44\ub3c4 \uc54c\uace0 \uc788\uc744 \uac70\ub77c\uace0 \uc0dd\uac01\ud574\uc694."
		},
		{
			"actor": 207030,
			"side": 2,
			"dir": 1,
			"nameColor": "#a9f548",
			"typewriter": {
				"speed": 0.05,
				"speedUp": 0.01
			},
			"painting": {
				"alpha": 0.3,
				"time": 1
			},
			"say": "\ud038 \uc5d8\ub9ac\uc790\ubca0\uc2a4 \ud3d0\ud558\uaed8\uc11c \uc548\ubd80\ub97c \uc804\ud574\ub2ec\ub77c \ud558\uc168\uc2b5\ub2c8\ub2e4. \ud3d0\ud558\uaed8\uc11c\ub294 \uc18c\ubb38\uc744 \ubbff\uae30 \ubcf4\ub2e4\ub294 \ub9f9\uc6b0\uc5d0\uac8c \uc9c1\uc811 \ub4e3\uaca0\ub2e4\uace0 \ub9d0\uc500\ud558\uc168\uc9c0\uc694."
		},
		{
			"say": "\ud6c4\ud6c4, \uadf8\ub7ec\uc168\uad70\uc694. \ud638\uc758\ub294 \uc798 \ubc1b\uc558\uc2b5\ub2c8\ub2e4.",
			"actor": 805010,
			"side": 2,
			"dir": 1,
			"nameColor": "#a9f548",
			"typewriter": {
				"speed": 0.05,
				"speedUp": 0.01
			},
			"painting": {
				"alpha": 0.3,
				"time": 1
			},
			"expression": 3
		},
		{
			"say": "\uc0ac\uc2e4 \uadf8 \uc18c\ubb38\uc740 \uadf8\ub2e4\uc9c0 \uacfc\uc7a5\ub41c \uac83\ub3c4 \uc544\ub2d9\ub2c8\ub2e4. \ud655\uc2e4\ud788 \uc774\ubc88 \uc791\uc804\uc5d0 \uac00\uc7a5 \uc911\uc694\ud55c \uac83\uc740 \uc774 \uc678\uc9c4 \uc601\ud1a0\uac00 \uc544\ub2c8\ub77c \uc774\uacf3\uc758 \uc5b4\ub5a4 '\ubb3c\uac74'\uc744 \ub418\ucc3e\ub294 \uac83\uc774\uc9c0\uc694.",
			"actor": 805010,
			"side": 2,
			"dir": 1,
			"nameColor": "#a9f548",
			"typewriter": {
				"speed": 0.05,
				"speedUp": 0.01
			},
			"painting": {
				"alpha": 0.3,
				"time": 1
			},
			"expression": 2
		},
		{
			"say": "\uc5ec\ub7ec\ubd84\ub3c4 \uc544\uc2dc\ub2e4\uc2dc\ud53c \uc774\uacf3\uc5d0\ub294 \uc624\ub798\ub41c \uc544\uc774\ub9ac\uc2a4 \ub9ac\ube0c\ub808\uc758 \uc131\ub2f9\uc774 \uc788\uc2b5\ub2c8\ub2e4. \uc131\ub2f9\uc744 \ub2e4\uc2dc \uc6b0\ub9ac\uc758 \uc190\uc5d0 \ub123\ub294 \uac83\uc774 \uc774\ubc88 \uc791\uc804\uc758 \ubaa9\ud45c\uc785\ub2c8\ub2e4.",
			"actor": 805010,
			"side": 2,
			"dir": 1,
			"nameColor": "#a9f548",
			"typewriter": {
				"speed": 0.05,
				"speedUp": 0.01
			},
			"painting": {
				"alpha": 0.3,
				"time": 1
			},
			"expression": 4
		},
		{
			"actor": 805010,
			"side": 2,
			"dir": 1,
			"nameColor": "#a9f548",
			"typewriter": {
				"speed": 0.05,
				"speedUp": 0.01
			},
			"painting": {
				"alpha": 0.3,
				"time": 1
			},
			"say": "\uadf8 \uc131\ub2f9\uc740 \uc6b0\ub9ac\uc5d0\uac8c \ub9e4\uc6b0 \uc911\uc694\ud55c \uc758\ubbf8\ub97c \uac00\uc9c0\uace0 \uc788\uc9c0\uc694. \uc131\ub2f9\uc744 \uc190\uc5d0 \ub123\ub294 \ucabd\uc774 \uc544\uc774\ub9ac\uc2a4 \uad50\uad6d\uc758 '\uc815\ud1b5\uc131'\uc744 \uacc4\uc2b9\ud55c\ub2e4\uace0 \ud574\ub3c4 \ubb34\ubc29\ud569\ub2c8\ub2e4."
		},
		{
			"say": "\ube44\uc2dc\uc544 \uc131\uc88c\ub3c4 \uac19\uc740 \uc774\uc720 \ub54c\ubb38\uc5d0 \uc774\ub840\uc801\uc73c\ub85c \uc774 \uc678\uc9c4 \uacf3\uae4c\uc9c0 \ud568\ub300\ub97c \ud30c\uacac\ud588\uc9c0\uc694.",
			"actor": 805010,
			"side": 2,
			"dir": 1,
			"nameColor": "#a9f548",
			"typewriter": {
				"speed": 0.05,
				"speedUp": 0.01
