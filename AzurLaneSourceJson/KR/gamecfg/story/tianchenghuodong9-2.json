		{
			"actor": 305050,
			"side": 2,
			"dir": -1,
			"nameColor": "#92fc63",
			"typewriter": {
				"speed": 0.05,
				"speedUp": 0.01
			},
			"painting": {
				"alpha": 0.3,
				"time": 1
			},
			"say": "\uadf8\ub7fc, \uc804\uc5d0 \ub9d0\ud588\ub358 \uc5f0\ud569 \ud568\ub300\uc758 \ucc28\uae30 \uae30\ud568\uc740\u2026\u2026"
		},
		{
			"actor": 305050,
			"side": 2,
			"dir": -1,
			"nameColor": "#92fc63",
			"typewriter": {
				"speed": 0.05,
				"speedUp": 0.01
			},
			"painting": {
				"alpha": 0.3,
				"time": 1
			},
			"say": "\ucc28\ud6c4 \uc544\ub9c8\uae30\uae09 \uc21c\uc591\uc804\ud568, \uc544\ub9c8\uae30\uc5d0\uac8c \uc704\uc784\ud558\ub3c4\ub85d \ud558\uaca0\ub2e4!"
		},
		{
			"actor": 305050,
			"side": 2,
			"dir": -1,
			"nameColor": "#92fc63",
			"typewriter": {
				"speed": 0.05,
				"speedUp": 0.01
			},
			"painting": {
				"alpha": 0.3,
				"time": 1
			},
			"say": "\uc55e\uc73c\ub85c \uc544\ub9c8\uae30\uac00 \uacfc\uac70 \ubbf8\uce74\uc0ac\ub2d8\uc73c\ub85c\ubd80\ud130 \ub0b4\ub824\uc628 \uc774 \uc911\uc694\ud55c \uc790\ub9ac\ub97c \ubb3c\ub824\ubc1b\uc544, \uc6b0\ub9ac \uc0ac\ucfe0\ub77c \uc5e0\ud30c\uc774\uc5b4\ub97c \uc774\ub04c\uc5b4\ub098\uac08 \uc218 \uc788\uae38 \ubc14\ub780\ub2e4!"
