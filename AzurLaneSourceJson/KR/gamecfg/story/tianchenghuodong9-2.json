				"speedUp": 0.01
			},
			"painting": {
				"alpha": 0.3,
				"time": 1
			},
			"say": "\uc774\ubc88 \uc5f0\uc2b5 \ud6c8\ub828\uc740 \uc544\ub9c8\uae30\uac00 \uc774\ub044\ub294 \ud64d\uad70\ub3c4 \uce74\uac00\uac00 \uc774\ub044\ub294 \uccad\uad70\ub3c4 \ubaa8\ub450 \uc798 \uc2f8\uc6cc\uc8fc\uc5c8\ub2e4."
