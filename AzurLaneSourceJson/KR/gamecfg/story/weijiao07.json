			"painting": {
				"alpha": 0.3,
				"time": 1
			},
			"say": "\uadf8\ub807\uaca0\uc9c0!"
		},
		{
			"actor": 202030,
			"side": 0,
			"dir": 1,
			"nameColor": "#92fc63",
			"typewriter": {
				"speed": 0.05,
				"speedUp": 0.01
			},
			"painting": {
				"alpha": 0.3,
				"time": 1
			},
			"say": "\uadf8\ub9ac\uace0\u2026\u2026\ub2e4, \ub2f9\uc2e0\uc774 \ubb34\uc0ac\ud574\uc11c\u2026\ub2e4, \ub2e4\ud589\uc774\uc5d0\uc694!"
		},
		{
			"actor": 202020,
			"side": 2,
			"dir": 1,
			"nameColor": "#92fc63",
			"typewriter": {
				"speed": 0.05,
				"speedUp": 0.01
			},
			"painting": {
				"alpha": 0.3,
				"time": 1
			},
			"say": "\uc5d0\uc774\uc7ad\uc2a4? \u2026\uc815\ub9d0\uc774\uc9c0, \uc194\uc9c1\ud558\uc9c0 \ubabb\ud558\ub2e4\ub2c8\uae50~ \ub108\ub3c4 \uc5ec\uc804\ud558\ub124, \uc5d0\uc774\uc7ad\uc2a4."
		}
