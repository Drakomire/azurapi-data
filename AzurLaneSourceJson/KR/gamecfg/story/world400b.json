				"speed": 0.05,
				"speedUp": 0.01
			},
			"painting": {
				"alpha": 0.3,
				"time": 1
			},
			"say": "\uc138\uc774\ub80c\uc758 \uc124\ube44\ub97c \ucc28\uc9c0\ud558\uace0, \uae30\uc220\uc744 \ud68c\uc218\ud558\ub824\ub294 \uac83 \uc544\ub2d0\uae4c\uc694? \ubbff\uc744 \uc218 \uc5c6\uc5b4\uc694."
