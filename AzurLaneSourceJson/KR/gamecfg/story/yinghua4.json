			"painting": {
				"alpha": 0.3,
				"time": 1
			},
			"blackBg": true,
			"say": "\ud558\uc9c0\ub9cc\u2026 \uc804\uc7c1\uc774 \uc77c\uc5b4\ub098\uba74, \uc0ac\ub78c\ub4e4\uc774 \uc794\ub729 \uc8fd\uace0 \ub9cc\ub2e4\u2026 \uc801\uc740 \ubb3c\ub860, \uc6b0\ub9ac \uc0ac\ucfe0\ub77c \uc5e0\ud30c\uc774\uc5b4\ub3c4\u2026\u2026"
