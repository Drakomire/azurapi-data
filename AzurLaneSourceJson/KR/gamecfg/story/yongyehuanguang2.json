			"dir": 1,
			"nameColor": "#a9f548",
			"dialogShake": {
				"speed": 0.09,
				"x": 10,
				"number": 2
			},
			"say": "\ubb34\uc2a8 \ub9d0\uc774\uc57c, \ubca8\ud30c\uc2a4\ud2b8\u2026 \ub098\ub3c4 \ud560 \ub54c\ub294 \uc81c\ub300\ub85c \ud560 \uc218 \uc788\uc5b4!",
			"typewriter": {
