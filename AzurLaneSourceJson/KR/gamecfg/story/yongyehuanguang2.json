			"side": 1,
			"dir": 1,
			"nameColor": "#a9f548",
			"actorName": "\ud5c8\ub9c8\uc774\uc624\ub2c8",
			"painting": {
				"alpha": 0.3,
				"time": 1
			},
			"bgName": "bg_hms_1"
		},
		{
			"say": "\ud558\ud558, \uc624\ub798\uac04\ub9cc\uc758 \ucd9c\uaca9 \uc784\ubb34\ub77c \uae34\uc7a5\ud588\ub098\uc694? \ud5c8\ub9c8\uc774\uc624\ub2c8\ub77c\uba74 \ubd84\uba85 \ubb38\uc81c\uc5c6\uc744 \uac70\uc608\uc694. \uac00\ub2a5\ud558\ub2e4\uba74 \uc6b0\ub9ac \uc5b8\ub2c8 \uc880 \uc798 \ucc59\uaca8\ub2ec\ub77c\uace0 \ubd80\ud0c1\ud574\uc57c\uaca0\ub294\uac78\uc694.",
			"typewriter": {
				"speed": 0.05,
				"speedUp": 0.01
			},
			"side": 0,
			"dir": 1,
			"nameColor": "#a9f548",
			"actorName": "\ubca8\ud30c\uc2a4\ud2b8",
			"painting": {
				"alpha": 0.3,
