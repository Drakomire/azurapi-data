		{
			"side": 0,
			"dir": 1,
			"nameColor": "#a9f548",
			"bgm": "story-richang",
			"say": "\ucd9c\ud56d\ud560 \uc2dc\uac04\uc774 \uac70\uc758 \ub2e4 \ub41c \uac83 \uac19\uad70\uc694. \uc900\ube44 \uc791\uc5c5\uc740 \uc5b4\ub54c\uc694, \ud5c8\ub9c8\uc774\uc624\ub2c8?",
			"typewriter": {
				"speed": 0.05,
				"speedUp": 0.01
