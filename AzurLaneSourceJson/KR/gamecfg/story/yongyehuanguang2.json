				"alpha": 0.3,
				"time": 1
			},
			"bgName": "bg_hms_1"
		},
		{
			"say": "\uc54c\uc558\uc5b4, \uc54c\uc558\uc5b4\u2026\uadf8\ub7ec\uba74 \uc791\uc804 \uc784\ubb34\ub97c \ub2e4\uc2dc \ud655\uc778\ud574\uc57c\uaca0\uc9c0\uff1f",
