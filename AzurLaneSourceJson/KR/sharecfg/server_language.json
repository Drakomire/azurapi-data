{
	"1": {
		"id": 1,
		"content": "\uacbd\uc21c\uc591\ud568\uc740 \ub300\uacf5\uc5d0 \ud2b9\ud654\ub418\uc5b4\uc788\uc5b4.",
		"limitation": -1
	},
	"2": {
		"id": 2,
		"content": "\uc804\ud568\uc740 \uac15\ub825\ud55c \ud3ec\uaca9\uc744 \ud560 \uc218 \uc788\uc9c0\ub9cc, \uc18d\ub825\uc774 \ube44\uad50\uc801 \ub290\ub9b0 \ud3b8\uc774\uc57c.",
		"limitation": -1
	},
	"3": {
		"id": 3,
		"content": "\uc804\ud568\uc758 \ud3ec\uaca9\uc744 \uc218\ub3d9\uc73c\ub85c \uc870\uc900\ud558\uba74, \ub300\ubbf8\uc9c0\uac00 \uc57d 20% \uc815\ub3c4 \uc624\ub97c\uc9c0\ub3c4!?",
		"limitation": -1
	},
	"4": {
		"id": 4,
		"content": "\uacf5\uc911\uc9c0\uc6d0\uc740 \ud654\uba74 \ub0b4\uc758 \uc801\uc758 \ud0c4\ub9c9\uc744 \ubaa8\ub450 \uc5c6\uc568 \uc218 \uc788\uc5b4.",
		"limitation": -1
	},
	"5": {
		"id": 5,
		"content": "\u300c\uc7a5\uac11\uacc4\uc5f4\u300d \uc2a4\ud0ac\uc740 \uc18c\uad6c\uacbd \ud3ec\uc758 \ub300\ubbf8\uc9c0\ub9cc \ubb34\ud6a8\ub85c \ud560 \uc218 \uc788\uc5b4. \ub300\uad6c\uacbd \ud3ec, \uc5b4\ub8b0, \ud3ed\ud0c4\uc758 \ub300\ubbf8\uc9c0\ub294 \ubb34\ud6a8\ub85c \ud560 \uc218 \uc5c6\uc5b4. ",
		"limitation": -1
	},
	"6": {
		"id": 6,
		"content": "\ud56d\uacf5\ubaa8\ud568\uc5d0 \ud0d1\uc7ac\ub418\ub294 \uc804\ud22c\uae30 \ud3b8\ub300\ub294 \uc801\uc758 \uacf5\uaca9 \ud3b8\ub300\ub97c \uc790\ub3d9\uc73c\ub85c \uc694\uaca9\ud574. \ud558\uc9c0\ub9cc \uc5f0\uc18d\uc73c\ub85c \ub0b4\ubcf4\ub0bc \uc218\ub294 \uc5c6\uc5b4.",
		"limitation": -1
	},
	"7": {
		"id": 7,
		"content": "\ub300\uacf5\ud654\uae30\ub294 \uc0ac\uac70\ub9ac(\uc810\uc120) \ub0b4\uc758 \ubaa8\ub4e0 \uc801\uae30\ub97c \uc790\ub3d9\uc73c\ub85c \uc694\uaca9\ud558\uba70, \uadf8 \ub300\ubbf8\uc9c0\ub294 \uc870\uc900\ud55c \uc694\uaca9 \ub300\uc0c1\uc758 \uc218\ub9cc\ud07c \ub098\ub204\uc5b4\uc838.",
		"limitation": -1
	},
	"8": {
		"id": 8,
		"content": "\uc801\uc758 \ud568\uc120 \ud639\uc740 \ud568\uc7ac\uae30\uac00 \ud654\uba74\uc758 \uc67c\ucabd \ub05d\uc5d0 \ub3c4\ub2ec\ud558\uba74, \uc544\uad70\uc758 \uc8fc\ub825 \ud568\ub300\uac00 \ub300\ubbf8\uc9c0\ub97c \uc785\uac8c \ub3fc.",
		"limitation": -1
	},
	"9": {
		"id": 9,
		"content": "\ub300\ubd80\ubd84\uc758 \uc804\ud568\uc740 140mm \uc774\ud558\uc758 \ud3ec\ub97c \ubd80\ud3ec\ub85c \uc7a5\ucc29\ud560 \uc218 \uc788\uc5b4.",
		"limitation": -1
	},
	"10": {
		"id": 10,
		"content": "\uc801\uc758 \ub300\ud615\ud568\uc740 \uc218\ub9ce\uc740 \ubd80\ud3ec\uc5d0 \uc758\ud574 \ubcf4\ud638\ubc1b\uace0 \uc788\uc5b4. \uac00\uae4c\uc6b4 \uac70\ub9ac\uc5d0\uc11c \ub1cc\uaca9\uc744 \uc2dc\ub3c4\ud560 \ub54c\ub294, \uc801\uc758 \uacf5\uaca9\uc5d0 \uc8fc\uc758\ud558\ub3c4\ub85d \ud574!",
		"limitation": -1
	},
	"11": {
		"id": 11,
		"content": "\uc7a5\uac11\uc774 \ub450\uaebc\uc6b4 \uc801\uc758 \ub300\ud615\ud568\uc5d0\ub294, \uc18c\uad6c\uacbd \uc8fc\ud3ec\ubcf4\ub2e4\ub294 \uc5b4\ub8b0\ub098 \ud56d\uacf5 \uc804\ub825\uc744 \uc774\uc6a9\ud558\uc5ec \uacf5\uaca9\uc744 \uc2dc\ub3c4\ud558\ub294 \uac8c \uc88b\uc544.",
		"limitation": -1
	},
	"12": {
		"id": 12,
		"content": "\ucca0\uac11\ud0c4 \uc18d\uc131\uc758 \ubb34\uae30\ub294 \uc911\uc7a5\uac11\uc744 \uac00\uc9c4 \uc801\uc5d0 \ub300\ud574 \ubcf4\ub2e4 \uac15\ub825\ud55c \ub300\ubbf8\uc9c0\ub97c \uc785\ud790 \uc218 \uc788\uc5b4.",
		"limitation": -1
	},
	"13": {
		"id": 13,
		"content": "\uace0\ud3ed\ud0c4 \uc18d\uc131\uc758 \ubb34\uae30\ub294 \uc77c\uc815 \ud655\ub960\ub85c \uc801\uc744 \uc5f0\uc18c \uc0c1\ud0dc\ub85c \ub9cc\ub4e4\uc5b4, \uc9c0\uc18d\uc801\uc73c\ub85c \ub300\ubbf8\uc9c0\ub97c \uc785\ud790 \uc218 \uc788\uc5b4.",
		"limitation": -1
	},
	"14": {
		"id": 14,
		"content": "\uac19\uc740 \uc885\ub958\uc758 \uc2a4\ud0ac \ud6a8\uacfc\ub294 \uae30\ubcf8\uc801\uc73c\ub85c \uc911\ucca9\ub418\uc9c0 \uc54a\uc544. \uac00\uc7a5 \ub808\ubca8\uc774 \ub192\uc740 \uc2a4\ud0ac\uc758 \ud6a8\uacfc\ub9cc \uc801\uc6a9\ubc1b\uc744 \uc218 \uc788\uc5b4.",
		"limitation": -1
	},
	"15": {
		"id": 15,
		"content": "\uc5f0\uc18c \uc0c1\ud0dc\uc5d0\uc11c \uace0\ud3ed\ud0c4 \uacf5\uaca9\uc744 \ubc1b\uc744 \uacbd\uc6b0, \uc5f0\uc18c \uc9c0\uc18d \uc2dc\uac04\uc774 \ub298\uc5b4\ub098\uba70, \ucd94\uac00\uc801\uc778 \ub300\ubbf8\uc9c0\ub97c \ubc1b\uc744 \uc218\ub3c4 \uc788\uc5b4.",
		"limitation": -1
	},
	"16": {
		"id": 16,
		"content": "\uc804\ud22c \uac1c\uc2dc \uc804\uc758 \ud3b8\uc131 \ud654\uba74\uc5d0\uc11c \uae30\ud568\uc744 \ubcc0\uacbd\ud560 \uc218 \uc788\uc5b4.",
		"limitation": -1
	},
	"17": {
		"id": 17,
		"content": "\ub2e8\uc885\uc9c4\uc740 \ud3ec\uaca9, \ubcf5\uc885\uc9c4\uc740 \uae30\ub3d9, \uc724\ud615\uc9c4\uc740 \ub300\uacf5 \ub4f1\uc5d0 \uac01\uac01 \ud2b9\ud654\ub418\uc5b4 \uc788\uc5b4.",
		"limitation": -1
	},
	"18": {
		"id": 18,
		"content": "\u300c\uc124\uc815\u300d\u2192\u300c\uc54c\ub9bc\u300d\u2192\u300c\uae30\ud0c0\u300d\uc5d0\uc11c, \u300c\ubcc0\uacbd\ub41c \uc774\ub984\u300d\uc758 \ud45c\uc2dc \uc5ec\ubd80\ub97c \uc124\uc815\ud560 \uc218 \uc788\uc5b4.",
		"limitation": -1
	},
	"19": {
		"id": 19,
		"content": "\uc790\uc728 \uc804\ud22c(\uc790\ub3d9 \uc804\ud22c)\ub294 1-3 \uc2a4\ud14c\uc774\uc9c0\ub97c \ud074\ub9ac\uc5b4\ud558\uba74 \uac1c\ubc29\ub3fc.",
		"limitation": -1
	},
	"20": {
		"id": 20,
		"content": "\ud6c8\ub828\uc5d0\uc11c \ubc1b\uc740 \uacf5\ud6c8 \uc810\uc218\ub294 \uc0c1\uc810\uc5d0\uc11c \ubcf4\uc0c1\uc73c\ub85c \uad50\ud658\ud560 \uc218 \uc788\uc5b4.",
		"limitation": -1
	},
	"21": {
		"id": 21,
		"content": "\uc2a4\ud0ac\uc740 \uacf5\uaca9(\uc801\uc0c9), \ubc29\uc5b4(\uccad\uc0c9), \uc9c0\uc6d0(\ud669\uc0c9)\uc73c\ub85c \ucd1d 3\uac00\uc9c0 \ud0c0\uc785\uc774 \uc788\uc5b4.",
		"limitation": -1
	},
	"22": {
		"id": 22,
		"content": "\uc219\uc18c\uc758 \ucf8c\uc801\ub3c4\ub294 \ubc30\uce58\ub418\uc5b4 \uc788\ub294 \uac00\uad6c\uac00 \uc544\ub2cc, \ubcf4\uc720\ud558\uace0 \uc788\ub294 \uac00\uad6c\uc5d0 \uc758\ud574 \uacb0\uc815\ub3fc.",
		"limitation": -1
	},
	"23": {
		"id": 23,
		"content": "\uc801\uc758 \uc218\uc1a1\ud568\uc740 \ubc29\uce58\ud574 \ub450\uba74 \uc804\uc7a5\uc5d0\uc11c \uc774\ud0c8\ud560 \uc218\ub3c4 \uc788\uc5b4. \ud654\ub825\uc744 \uc9d1\uc911\ud574!",
		"limitation": -1
	},
	"24": {
		"id": 24,
		"content": "\ucee8\ub514\uc158\uc774 \ub0ae\uc740 \uc0c1\ud0dc(\uc801\uc0c9 \uc544\uc774\ucf58)\uc5d0\uc11c \ucd9c\uaca9\ud558\uba74, \ud68d\ub4dd \uacbd\ud5d8\uce58\uac00 \uc808\ubc18\uc73c\ub85c \uc904\uc5b4\ub4e4\uac8c \ub3fc\u2026",
		"limitation": -1
	},
